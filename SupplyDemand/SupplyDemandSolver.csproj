<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
	  <TargetFramework>net452</TargetFramework>
	  <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  <Authors>dirkju@outlook.com</Authors>
	  <Description>SupplyDemandSolver provides a simple Supply Demand matching algorithm. It supports supplier capacity, demand priorities for different suppliers as well as constraint categories to limit matching.</Description>
	  <Copyright>(c) Dirk Junghanns 2022</Copyright>
	  <PackageTags>Supply, Demand, Solver, Matching</PackageTags>
	  <RepositoryUrl>https://github.com/dirkju/SupplyDemand</RepositoryUrl>
	  <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
	  <Version>1.1.5</Version>
	  <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
  <ItemGroup>
    <None Update="README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
</Project>